<Window x:Class="Project_Zeus.single_user"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Project_Zeus"
        mc:Ignorable="d"
        Title="single_user" 
        WindowStartupLocation="CenterScreen"
        Width="{DynamicResource {x:Static SystemParameters.MaximizedPrimaryScreenWidthKey}}" 
        Height="{DynamicResource {x:Static SystemParameters.MaximizedPrimaryScreenHeightKey}}">
    <Window.Background>
        <ImageBrush ImageSource="image/dental-center-header.jpg"/>
    </Window.Background>
    <Grid>
        <Button Content="Register New Patient" HorizontalAlignment="Left" Margin="34,711,0,0" VerticalAlignment="Top" Width="125" Click="Button_Click_2"/>
        <Button Content="update password" HorizontalAlignment="Left" Margin="34,761,0,0" VerticalAlignment="Top" Width="125" Click="Button_Click_1" />
        <Button Content="logout" HorizontalAlignment="Left" Margin="34,811,0,0" VerticalAlignment="Top" Width="125" Click="Button_Click"/>
        <Frame x:Name="adminFrame" NavigationUIVisibility="Hidden" Content="" HorizontalAlignment="Left" Height="600" Margin="300,0,0,0" VerticalAlignment="Top" Width="1000"/>
    </Grid>
</Window>
